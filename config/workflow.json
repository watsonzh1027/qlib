{
    "data":
    {
        "csv_data_dir": "data/klines",
        "bin_data_dir": "data/qlib_data/crypto",
        "symbols": "config/top50_symbols.json"
    },
  "data_collection": {
    "interval": "1m",
    "api_timeout": 10,
    "start_time": "2025-11-01",
    "end_time": "",
    "limit": 1000,
    "overlap_minutes": 5,
    "enable_incremental": true
  },
  "data_convertor": {
    "date_field_name": "timestamp",
    "include_fields": ["open", "high", "low", "close", "volume"],
    "instruments_file": "instruments.txt"
  },
  "backtest": {
    "start_time": "2025-11-01",
    "end_time": "2025-11-08",
    "account": 1000000,
    "benchmark": "BTCUSDT",
    "exchange_kwargs": {
      "freq": "1min",
      "limit_threshold": null,
      "deal_price": "close",
      "open_cost": 0.001,
      "close_cost": 0.001,
      "min_cost": 0.001
    }
  },
  "workflow": {
    "start_time": "2025-11-01",
    "end_time": "2025-11-08",
    "frequency": "1m",
    "instruments_limit": 50
  },
  "model": {
    "type": "GBDT",
    "learning_rate": 0.1,
    "num_boost_round": 100
  },
  "model_config_full": {
    "class": "LGBModel",
    "module_path": "qlib.contrib.model.gbdt",
    "kwargs": {
      "loss": "mse",
      "colsample_bytree": 0.8879,
      "learning_rate": 0.1,
      "subsample": 0.8789,
      "lambda_l1": 205.6999,
      "lambda_l2": 580.9768,
      "max_depth": 8,
      "num_leaves": 210,
      "num_threads": 20,
      "num_boost_round": 100
    }
  },
  "data_handler_config": {
    "class": "DataHandlerLP",
    "module_path": "qlib.data.dataset.handler",
    "kwargs": {
      "start_time": "<workflow.start_time>",
      "end_time": "<workflow.end_time>",
      "instruments": "<data.symbols>",
      "data_loader": {
        "class": "QlibDataLoader",
        "kwargs": {
          "config": {
            "feature": [
              "$open",
              "$high", 
              "$low",
              "$close",
              "$volume"
            ],
            "label": ["$close"]
          },
          "freq": "1min"
        }
      }
    }
  },
  "trading": {
    "open_cost": 0.001,
    "close_cost": 0.001,
    "min_cost": 0.001,
    "strategy_topk": 50,
    "strategy_n_drop": 5
  },
  "dataset": {
    "class": "DatasetH",
    "module_path": "qlib.data.dataset",
    "kwargs": {
      "handler": "<data_handler_config>",
      "segments": {
        "train": ["2025-11-01", "2025-11-03"],
        "valid": ["2025-11-03", "2025-11-05"],
        "test": ["2025-11-05", "<workflow.end_time>"]
      }
    }
  },
  "port_analysis": {
    "executor": {
      "class": "SimulatorExecutor",
      "module_path": "qlib.backtest.executor",
      "kwargs": {
        "time_per_step": "1min",
        "generate_portfolio_metrics": true
      }
    },
    "strategy": {
      "class": "TopkDropoutStrategy",
      "module_path": "qlib.contrib.strategy.signal_strategy",
      "kwargs": {
        "topk": "<trading.strategy_topk>",
        "n_drop": "<trading.strategy_n_drop>"
      }
    },
    "backtest": {
      "start_time": "<backtest.start_time>",
      "end_time": "<backtest.end_time>",
      "account": "<backtest.initial_capital>",
      "benchmark": null,
      "exchange_kwargs": {
        "freq": "1min",
        "limit_threshold": null,
        "deal_price": "close",
        "open_cost": "<trading.open_cost>",
        "close_cost": "<trading.close_cost>",
        "min_cost": "<trading.min_cost>"
      }
    }
  }
}