[run]
source = 
    qlib
    examples
branch = True
omit = 
    */tests/*
    */__init__.py
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise NotImplementedError
    pass
    raise ImportError

# Show missing lines in the report
show_missing = True
skip_covered = False

[html]
directory = coverage_html
show_contexts = True
title = QLib Test Coverage Report
